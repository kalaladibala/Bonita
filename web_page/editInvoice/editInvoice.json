    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.invoice ){",
        "\treturn {",
        "\t\t//map invoice variable to expected task contract input",
        "\t\tinvoiceListInput: $data.invoice.map( it => ({",
        "\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\tnumber: it.number !== undefined ? it.number : null,",
        "\t\t\tdateInvoice: it.dateInvoice !== undefined ? it.dateInvoice : null,",
        "\t\t\ttotal: it.total !== undefined ? it.total : null,",
        "\t\t\tprovider: it.provider ? {",
        "\t\t\t\tpersistenceId_string: it.provider.persistenceId_string !== undefined ? it.provider.persistenceId_string : null",
        "\t\t\t} : null,",
        "\t\t\tinvoiceLines: it.invoiceLines.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\t\tproduct: it.product !== undefined ? it.product : null,",
        "\t\t\t\tprice: it.price !== undefined ? it.price : null",
        "\t\t\t}))",
        "\t\t}))",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
